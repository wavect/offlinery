{
    "name": "offlinery",
    "version": "1.0.0",
    "main": "expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "devtools": "npx react-devtools",
        "start:dev": "npx concurrently \"pnpm devtools\" \"EXPO_PUBLIC_ENVIRONMENT=development pnpm start\"",
        "start:devwin": "npx concurrently \"pnpm devtools\" \"set EXPO_PUBLIC_ENVIRONMENT=development && pnpm start\"",
        "start:prod": "expo start --no-dev --minify",
        "start:tunnel": "expo start --tunnel",
        "android": "expo run:android",
        "ios": "EXPO_PUBLIC_ENVIRONMENT=development expo run:ios",
        "ios:device": "EXPO_PUBLIC_ENVIRONMENT=development expo run:ios --device",
        "ios:dev": "EXPO_PUBLIC_ENVIRONMENT=development expo run:ios",
        "ios:clean": "rm -rf ios/Pods && rm -rf ios/build",
        "ios:expo:build": "eas build --platform ios",
        "ios:expo:submit": "eas submit -p ios --latest",
        "web": "expo start --web",
        "install:all": "pnpm i && npx expo install --fix",
        "pre-commit": "npx prettier . --write",
        "e2e:ios:build": "expo run:ios --configuration Release",
        "e2e:ios": "EXPO_PUBLIC_ENVIRONMENT=development maestro test  --debug-output=true -e APP_PATH=ios/build/Build/Products/Debug-iphonesimulator/Offlinery.app -e EXPO_PUBLIC_ENVIRONMENT=development ./tests/e2e",
        "e2e:ios:single": "EXPO_PUBLIC_ENVIRONMENT=development maestro test -e APP_PATH=ios/build/Build/Products/Debug-iphonesimulator/Offlinery.app -e EXPO_PUBLIC_ENVIRONMENT=development ./tests/e2e/basic-location-updates.e2e.yaml"
    },
    "dependencies": {
        "@expo-google-fonts/montserrat": "^0.2.3",
        "@expo/html-elements": "^0.10.1",
        "@expo/metro-config": "~0.19.4",
        "@expo/metro-runtime": "~4.0.0",
        "@expo/vector-icons": "^14.0.3",
        "@ptomasroos/react-native-multi-slider": "^2.2.2",
        "@react-native-async-storage/async-storage": "1.23.1",
        "@react-native-community/datetimepicker": "8.2.0",
        "@react-native-community/slider": "4.5.5",
        "@react-native-firebase/app": "^21.2.0",
        "@react-navigation/bottom-tabs": "^6.6.1",
        "@react-navigation/native": "^6.1.18",
        "@react-navigation/stack": "^6.4.1",
        "@sentry/react-native": "^6.1.0",
        "expo": "~52.0.11",
        "expo-build-properties": "~0.13.1",
        "expo-checkbox": "~4.0.0",
        "expo-constants": "^17.0.3",
        "expo-dev-client": "~5.0.4",
        "expo-device": "~7.0.1",
        "expo-font": "~13.0.1",
        "expo-gradle-ext-vars": "^0.1.2",
        "expo-image-manipulator": "~13.0.5",
        "expo-image-picker": "~16.0.3",
        "expo-linear-gradient": "~14.0.1",
        "expo-localization": "~16.0.0",
        "expo-network": "~7.0.2",
        "expo-notifications": "~0.29.8",
        "expo-splash-screen": "~0.29.13",
        "expo-status-bar": "~2.0.0",
        "expo-task-manager": "~12.0.3",
        "expo-updates": "~0.26.9",
        "i18n-js": "^4.4.3",
        "jwt-decode": "^4.0.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-hook-form": "^7.53.1",
        "react-native": "0.76.3",
        "react-native-background-fetch": "^4.2.6",
        "react-native-background-geolocation": "^4.17.6",
        "react-native-element-dropdown": "^2.12.1",
        "react-native-error-boundary": "^1.2.4",
        "react-native-gesture-handler": "^2.20.2",
        "react-native-maps": "1.18.0",
        "react-native-mmkv": "^3.1.0",
        "react-native-ratings": "^8.1.0",
        "react-native-reanimated": "3.16.1",
        "react-native-reanimated-carousel": "^3.5.1",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "4.1.0",
        "react-native-swiper-flatlist": "^3.2.5",
        "react-native-web": "~0.19.13",
        "react-native-webview": "13.12.2"
    },
    "devDependencies": {
        "@expo/ngrok": "^4.1.3",
        "@types/react": "~18.3.12",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.0.0",
        "typescript": "^5.3.3"
    },
    "private": true
}
