{
    "name": "offlinery",
    "version": "1.0.0",
    "main": "expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "devtools": "npx react-devtools",
        "storybook:ios": "EXPO_PUBLIC_STORYBOOK=true pnpm ios:device",
        "storybook:android": "EXPO_PUBLIC_STORYBOOK=true pnpm android",
        "storybook:win": "set EXPO_PUBLIC_STORYBOOK=true && set EXPO_PUBLIC_ENVIRONMENT=development && npx expo run:android",
        "start:dev": "npx concurrently \"pnpm devtools\" \"EXPO_PUBLIC_ENVIRONMENT=development pnpm start\"",
        "start:devwin": "npx concurrently \"pnpm devtools\" \"set EXPO_PUBLIC_ENVIRONMENT=development && pnpm start\"",
        "start:prod": "expo start --no-dev --minify",
        "start:tunnel": "expo start --tunnel",
        "android": "npx concurrently \"EXPO_PUBLIC_ENVIRONMENT=development expo run:android\" \"npx react-native log-android\"",
        "android:emulator": "EXPO_PUBLIC_USE_EMULATOR=true pnpm android",
        "ios": "EXPO_PUBLIC_ENVIRONMENT=development expo run:ios",
        "ios:device": "EXPO_PUBLIC_ENVIRONMENT=development expo run:ios --device",
        "ios:dev": "EXPO_PUBLIC_ENVIRONMENT=development expo run:ios",
        "ios:clean": "rm -rf ios/Pods && rm -rf ios/build",
        "ios:expo:build": "eas build --platform ios",
        "ios:expo:submit": "eas submit -p ios --latest",
        "web": "expo start --web",
        "install:all": "pnpm i && npx expo install",
        "install:all:fix": "pnpm i && npx expo install --fix",
        "pre-commit": "npx prettier . --write",
        "e2e:ios:build": "expo run:ios --configuration Release",
        "e2e:ios": "EXPO_PUBLIC_ENVIRONMENT=development maestro test  --debug-output=true -e APP_PATH=ios/build/Build/Products/Debug-iphonesimulator/Offlinery.app -e EXPO_PUBLIC_ENVIRONMENT=development ./tests/e2e",
        "e2e:ios:single": "EXPO_PUBLIC_ENVIRONMENT=development maestro test -e APP_PATH=ios/build/Build/Products/Debug-iphonesimulator/Offlinery.app -e EXPO_PUBLIC_ENVIRONMENT=development ./tests/e2e/basic-location-updates.e2e.yaml",
        "storybook-generate": "sb-rn-get-stories",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "@expo-google-fonts/montserrat": "^0.2.3",
        "@expo/html-elements": "^0.11.2",
        "@expo/metro-config": "^0.19.9",
        "@expo/metro-runtime": "^4.0.1",
        "@expo/vector-icons": "^14.0.4",
        "@ptomasroos/react-native-multi-slider": "^2.2.2",
        "@react-native-async-storage/async-storage": "^1.23.1",
        "@react-native-community/datetimepicker": "^8.2.0",
        "@react-native-community/slider": "^4.5.5",
        "@react-native-firebase/app": "^21.7.1",
        "@react-navigation/bottom-tabs": "^7.2.0",
        "@react-navigation/native": "^7.0.14",
        "@react-navigation/stack": "^7.1.1",
        "@sentry/react-native": "^6.3.0",
        "@testing-library/react-native": "^13.0.1",
        "expo": "^52.0.28",
        "expo-build-properties": "^0.13.2",
        "expo-checkbox": "^4.0.1",
        "expo-constants": "^17.0.5",
        "expo-dev-client": "^5.0.10",
        "expo-device": "^7.0.2",
        "expo-font": "^13.0.3",
        "expo-gradle-ext-vars": "^0.1.2",
        "expo-image-manipulator": "^13.0.6",
        "expo-image-picker": "^16.0.4",
        "expo-linear-gradient": "^14.0.2",
        "expo-localization": "^16.0.1",
        "expo-network": "^7.0.5",
        "expo-notifications": "^0.29.13",
        "expo-quick-actions": "^4.0.2",
        "expo-splash-screen": "^0.29.21",
        "expo-status-bar": "^2.0.1",
        "expo-task-manager": "^12.0.5",
        "expo-updates": "^0.26.13",
        "i18n-js": "^4.5.1",
        "jwt-decode": "^4.0.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.54.2",
        "react-native": "^0.76.6",
        "react-native-background-fetch": "^4.2.7",
        "react-native-background-geolocation": "^4.18.3",
        "react-native-element-dropdown": "^2.12.4",
        "react-native-error-boundary": "^1.2.7",
        "react-native-gesture-handler": "^2.20.2",
        "react-native-maps": "^1.18.0",
        "react-native-mmkv": "^2.12.2",
        "react-native-ratings": "^8.1.0",
        "react-native-reanimated": "^3.16.7",
        "react-native-reanimated-carousel": "^3.5.1",
        "react-native-safe-area-context": "^4.12.0",
        "react-native-screens": "^4.4.0",
        "react-native-swiper-flatlist": "^3.2.5",
        "react-native-web": "^0.19.13",
        "react-native-webview": "^13.12.5",
        "rn-tourguide": "^3.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.26.7",
        "@expo/ngrok": "^4.1.3",
        "@gorhom/bottom-sheet": "^5.0.6",
        "@react-native-community/cli": "^15.1.3",
        "@storybook/addon-ondevice-actions": "^8.5.2",
        "@storybook/addon-ondevice-backgrounds": "^8.5.2",
        "@storybook/addon-ondevice-controls": "^8.5.2",
        "@storybook/addon-ondevice-notes": "^8.5.2",
        "@storybook/react": "^8.5.2",
        "@storybook/react-native": "^8.5.2",
        "@types/lodash.debounce": "^4.0.9",
        "@types/react": "^18.3.18",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.2.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-expo": "^52.0.3",
        "lodash.debounce": "^4.0.8",
        "prettier": "^3.4.2",
        "prettier-plugin-organize-imports": "^4.1.0",
        "react-native-svg": "^15.8.0",
        "react-test-renderer": "^18.2.0",
        "storybook-addon-i18n": "^5.1.13",
        "typescript": "^5.7.3"
    },
    "private": true
}
