{
  "cli": {
    "version": ">= 11.0.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "https://offlinery.onrender.com/v1"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "https://offlinery.onrender.com/v1"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://offlinery.onrender.com/v1"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
