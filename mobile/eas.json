{
    "cli": {
        "version": ">= 11.0.3",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "API_URL": "https://offlinery.onrender.com/v1"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "API_URL": "https://offlinery.onrender.com/v1"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "API_URL": "https://offlinery.onrender.com/v1"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
