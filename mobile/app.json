            "bundleIdentifier": "io.wavect.offlinery"
        },
        "android": {
            "versionCode": 3,
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyCvQCDDpKQW-DlxC44t8edMgC52HvTl4LM"
                }
            },
            "googleServicesFile": "./google-services.json",
            "permissions": [
                "android.permission.CAMERA",
                "android.permission.READ_MEDIA_IMAGES",
                "android.permission.READ_EXTERNAL_STORAGE",
                "android.permission.RECORD_AUDIO",
                "android.permission.ACCESS_COARSE_LOCATION",
                "android.permission.ACCESS_FINE_LOCATION",
                "android.permission.ACCESS_BACKGROUND_LOCATION",
                "android.permission.FOREGROUND_SERVICE",
                "android.permission.FOREGROUND_SERVICE_LOCATION",
                "android.permission.POST_NOTIFICATIONS",
                "android.permission.RECEIVE_BOOT_COMPLETED",
                "android.permission.WAKE_LOCK"
            ],
            "package": "io.wavect.offlinery"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            ["expo-font"],
            [
                "expo-notifications",
                {
                    "icon": "./assets/icon.png",
                    "color": "#459da1",
                    "androidMode": "default",
                    "androidCollapsedTitle": "Updates from Offlinery"
                }
            ],
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let others know about your appearance."
                }
            ],
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "$(PRODUCT_NAME)  needs your location to show you interesting people nearby when you're using the app.",
                    "locationAlwaysPermission": "$(PRODUCT_NAME)  needs your location to show you interesting people nearby when you're using the app.",
                    "locationWhenInUsePermission": "$(PRODUCT_NAME)  needs your location to show you interesting people nearby when you're using the app.",
