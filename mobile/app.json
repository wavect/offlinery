            "config": {
                "googleMapsApiKey": "AIzaSyCvQCDDpKQW-DlxC44t8edMgC52HvTl4LM",
                "usesNonExemptEncryption": false
            },
            "infoPlist": {
                "UIBackgroundModes": ["location"],
