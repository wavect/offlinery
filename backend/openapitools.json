{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.7.0",
    "generators": {
      "offlinery-api": {
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/src/api",
        "inputSpec": "#{cwd}/swagger.json",
        "additionalProperties": {
          "npmName": "@offlinery/api",
          "supportsES6": true,
          "withInterfaces": true,
          "typescriptThreePlus": true,
          "useSingleRequestParameter": true,
          "useObjectParameters": true,
          "enumPropertyNaming": "original",
          "modelPropertyNaming": "original",
          "useSingleEnumType": true,
          "stringEnums": false,
          "apiPackage": "api",
          "modelPackage": "model",
          "httpUserAgent": "Offlinery-API-Client",
          "usePromises": true
        }
      }
    }
  }
}